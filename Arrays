//Printing out elements of an array with a for loop

var cities = ["Melbourne", "Venice", "Rome" , "New York City", "Prague", "London", "Hong Kong"];

for (var i 0; i< cities.length; i++){ //notice how calling length calls the number of itmes within an array
  console.log("I would like to visit" + " " cities[i]);
}
